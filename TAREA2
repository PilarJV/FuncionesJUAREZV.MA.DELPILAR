#include <stdio.h>
#include <stdbool.h>// Boolean
long factorial(int n)
void simpson(); //Declaración de función

{
	if (n == 0) /* caso base */
		return 1; /* como 0! = 1, regresa 1*/
	else
		return n * factorial (n - 1); /* llamada a esta misma función */
}

int main(void)
{
	/* en este caso se llama a la función y se imprime directamente*/
	printf("%ld\n ", factorial(-10));

	return 0;
}


void simpson(int dia){ //Implementación de función
    if(dia <= 6){
	printf("Fase 1\n");
    }
     
    else{
	printf("Fase 3\n");
    }
